[application]

config/name="Backwords"
run/main_scene="res://Scenes/MainMenu.tscn"
boot_splash/image="res://Graphics/Logos/cowboyscott.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

PuzzleGeneratorSingleton="*res://Puzzles/PuzzleGenerator.gd"
